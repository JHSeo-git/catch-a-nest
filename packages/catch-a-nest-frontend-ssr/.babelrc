{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ],
    "@emotion/babel-preset-css-prop"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@components": "./components",
          "@lib": "./lib"
        }
      }
    ],
    "@emotion/babel-plugin"
  ]
}
